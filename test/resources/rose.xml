<project>

    <meta>
        <name>Rose</name>
        <!-- The name of the src. Should be in title case. Will
        have spaces removed & capitalization added to generate the
        root folder name. For example "Project Creator" would become
        the folder "ProjectCreator"-->
        <root_dir>/Users/Michael/prog/python/python3</root_dir>
        <date>
            <month>11</month>
            <day>19</day>
            <year>2018</year>
        </date>
        <license>MIT</license>
        <contributors>Michael Newman</contributors>

        <short_description>
            Generic python utility scripts
        </short_description>

        <long_description>A collection of python scripts useful to many
            programs.
        </long_description>
        <keyords>
            <k>utility</k>
            <k>python</k>
        </keyords>
    </meta>

    <language>
        <type>Python</type>
        <version>3.7</version>
        <frameworks/>
        <libraries/>
        <requirements/>
    </language>

    <test>
        <import/>
        <frameworks/>
        <libraries/>
        <requirements>
            <r>unittest</r>
        </requirements>
    </test>

    <auto_generate>
        <text_files>requirements</text_files>
        <rst_files>readme, license, contributors</rst_files>
        <folders>test</folders>
    </auto_generate>

    <git>
        <username/>
        <password/>
        <branches>
            <branch/>
        </branches>
        <remotes>
            <remote/>
        </remotes>
        <import>
            <url/>
        </import>
    </git>

    <sync/>

    <!--~~~~~~~~~~~~~~~~~~~~~~~~~~~~~HIERARCHY~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~-->
    <folder_root>
        <src>
            <meta type="folder"/>
            <sync>
                <git track="True"/>
            </sync>
            <children>
                <strings>
                    <meta type="module" extension="py">
                        <language>python</language>
                        <version>3.7</version>
                    </meta>
                    <contents>
                        <methods>
                            <require_string>
                                <param>
                                    <obj/>
                                    <log default="None"/>
                                </param>
                                <return type="str"/>
                            </require_string>
                        </methods>
                    </contents>
                </strings>
            </children>
        </src>

        <venv>
            <meta type="folder"/>
            <sync>
                <git track="False"/>
            </sync>
        </venv>

        <docs>
            <meta type="folder"/>
        </docs>

        <resources>
            <meta type="folder"/>
        </resources>

        <logs>
            <meta type="folder"/>
            <sync>
                <git track="False"/>
            </sync>
        </logs>

        <design>
            <meta type="folder"/>
            <children>
                <agile>
                    <meta type="folder"/>
                    <children>
                        <origion_backlog>
                            <meta type="file" extension="rst"/>
                            <contents>
                                <header>Project Backlog</header>
                            </contents>
                        </origion_backlog>

                        <sprint_backlog type="file">
                            <meta type="file" extension="rst"/>
                            <contents>
                                <text>[]. design</text>
                            </contents>
                        </sprint_backlog>
                    </children>

                </agile>
                <plan>
                    <meta type="file" extension="rst"/>
                </plan>
            </children>
        </design>
    </folder_root>
</project>